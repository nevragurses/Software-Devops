{  "$schema": "http://json-schema.org/draft-04/schema#",
       "title": "Request information for monitoring module",
       "type": "object",
       "description": "Contains required and unrequired data fields for comunication with monitoring module. Important: object_type = monitoring_request",
       "properties": {
       	 "object_type": {"type": "string"},
         "method": { "type": "string" },
         "app_name": { "type": "string" },
         "app_path": { "type": "string" },
         "public_ip": { "type": "string" },
         "pem_file": { "type": "string" },
         "usage": { "type": "string" }
       },
       "required": [ "object_type", "method", "app_name", "public_ip" ]
}