{  "$schema": "http://json-schema.org/draft-04/schema#",
       "title": "Response information for all modules",
       "type": "object",
       "description": "Contains method and its execution status with description for responses between all modules. Important: object_type = response",
       "properties": {
         "object_type": {"type": "string"},
         "method": { "type": "string" },
         "status": { "type": "string" },
         "description": { "type": "string" },
         "commit_id": {"type": "string"},
         "github_login": {"type": "string"},
         "github_password": {"type": "string"},
         "repository_url": {"type": "string"},
         "project_name": {"type": "string"},
         "trello_api": { "type": "string" },
         "trello_token": { "type": "string" }
       },
       "required": [ "object_type", "method", "status" ]
}